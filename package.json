{
  "name": "ipt",
  "version": "1.1.0",
  "description": "iPipeTo - The missing shell interactive workflow",
  "repository": "ruyadorno/ipt",
  "author": {
    "name": "Ruy Adorno",
    "url": "ruyadorno.com"
  },
  "bin": "src/cli.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "pretest": "xo",
    "test": "ava --tap"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "ipipeto",
    "pipe",
    "interactive",
    "list",
    "inquirer",
    "unix",
    "terminal",
    "workflow",
    "cli-app",
    "cli",
    "ipt",
    "menu"
  ],
  "dependencies": {
    "clipboardy": "^1.2.2",
    "extract-path": "^1.0.0",
    "fuzzysearch": "^1.0.3",
    "get-stdin": "^6.0.0",
    "inquirer": "^5.0.1",
    "inquirer-autocomplete-prompt": "^0.12.1",
    "reopen-tty": "^1.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "lodash.template": "^4.4.0",
    "mock-require": "^3.0.1",
    "tempfile": "^2.0.0",
    "xo": "^0.20.3"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false
  },
  "xo": {
    "rules": {
      "no-negated-condition": 0,
      "ava/test-ended": 0,
      "ava/prefer-async-await": 0,
      "promise/prefer-await-to-then": 0
    }
  },
  "license": "MIT"
}
